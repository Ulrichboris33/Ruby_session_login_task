
<h1>Login Page</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= f.label :email, "Email address" %>
  <%= f.email_field :email %>
  <br>
  <br>
  <%= f.label :password, "Password" %>
  <%= f.password_field :password %>
  <br>
  <br>
  <%= f.submit "Login", id: "create-session" %>
<% end %>